    <style name="EditTextStyle">
        <item name="android:textSize">13dp</item>
        <item name="android:fontFamily">@font/roboto</item>
        <!--<item name="android:background">@drawable/white_bg</item>-->
        <item name="android:gravity">center</item>
        <item name="android:focusable">true</item>
        <item name="android:inputType">number</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="TextStylesInput">
        <item name="android:textSize">11dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">@font/roboto</item>
        <!--<item name="android:background">@color/hint_color</item>-->
        <item name="android:gravity">center</item>
    </style>

    <style name="TextStylesHeadingWithoutBackgroundColor">
        <item name="android:textSize">10sp</item>
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:textColor">@color/gray_color</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="TextStylesHeading">
        <item name="android:textSize">10sp</item>
        <item name="android:fontFamily">@font/roboto</item>
        <!--<item name="android:background">@color/hint_color</item>-->
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
    </style>

    <style name="AllGameTopDesign">
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
        <!--<item name="android:background">@color/selected_tab_color</item>-->
        <item name="android:orientation">vertical</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="AllGameBottomDesign">
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>

    <style name="TextStylesNominalInput">
        <item name="android:textSize">13dp</item>
        <item name="android:textStyle">bold</item>
        <!--<item name="android:textCursorDrawable">@color/dark_color1</item>-->
        <item name="android:gravity">center</item>
        <!--<item name="android:hint">@string/nominal_txt</item>-->
        <!--<item name="android:background">@drawable/white_bg</item>-->
        <item name="android:focusable">true</item>
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:inputType">number</item>
        <item name="android:singleLine">true</item>
    </style>

